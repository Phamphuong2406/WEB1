<style>
    .custom-col {
        flex: 0 0 20%; 
        max-width: 20%;
        padding: 10px;
    }

    .partner-card {
        background-color: #fff;
        border-radius: 15px;
        padding: 20px;
        box-shadow: 0 10px 30px rgba(0, 0, 0, 0.05);
        display: flex;
        align-items: center;
        justify-content: center;
        margin-bottom: 20px;
    }
</style>


@model GeneralDesription
@{
    var listt = Model.partners;
    var list = Model.partners.ToList();
    var col1 = list.Take(1).ToList();//pt1
    var col2 = list.Skip(1).Take(2).ToList();//pt 2,3
    var col3 = list.Skip(3).Take(3).ToList();//pt 4,5,6
    var col4 = list.Skip(6).Take(2).ToList();//pt 7,9
    var col5 = list.Skip(8).Take(1).ToList();//pt 9
    var columns = new List<List<Partners> >{ col1, col2, col3, col4, col5 };
}
<section class="clients ">
    <div class="container">
        <div class="d-flex justify-content-between flex-wrap">
           
            @Html.Partial("_PartnersTitlePatial", Model)

            @foreach (var column in columns)
            {
                <div class="custom-col d-flex flex-column justify-content-center">
                    @foreach (var partner in column)
                    {
                        <div class="partner-card ">
                        @Html.Partial("_PartnersPatial", partner)
                        </div>
                    }
                </div>
            }

          


        </div>
    </div>
</section>
