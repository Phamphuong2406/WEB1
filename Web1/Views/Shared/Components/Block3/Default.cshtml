@model IEnumerable<Web1.Models.Block3Title>

<section class="clients section-padding" Style="Background:Red ;width: 100%; height: 400px;margin:100px">
    <div>
        <div class="row align-items-center">

            @foreach (var item in Model)
            {
                @Html.Partial("_Block3TitlePatial", item)
               
                @foreach (var block3 in item.blocks)
                {
                   
                    <div class="col-lg-2 col-3 ms-5 clients-item-height">
                        <div class="d-flex align-items-center">
                            <img src="~/images/@item.Icon" class="clients-image img-fluid me-4">
                            @Html.Partial("_Block3Patial", block3)
                           
                        </div>

                    </div>

                }
            }
        </div>
    </div>
</section>